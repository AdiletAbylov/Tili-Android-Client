<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="{'Результат: '  + data.keyword}">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:navigationContent>
		<s:Button label="Назад" click="{navigator.popView()}"/>
	</s:navigationContent>
	<s:List left="0" right="0" top="0" bottom="0" dataProvider="{data.result}">
		<s:itemRenderer>
			<fx:Component>
				<s:IconItemRenderer
					labelField="dictname" messageField="value" />
			</fx:Component>
		</s:itemRenderer>
	</s:List>
</s:View>
